#include <iostream>
using namespace std;


void tambahDepan(int databaru1){
Node *baru;
    baru = new Node();
    baru->data = databaru;
    baru->next = baru;

    if (isEmpty())
    {
        head = baru;
        head->next = head;
    }
    else
    {
        Node *bantu;
        bantu = head;
        while (bantu->next != head)
        {
            bantu = bantu->next;
        }
        baru->next = head;
        head = baru;
        bantu->next = head;
    }


}


